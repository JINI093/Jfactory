{
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css|wasm|woff|woff2)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "j-factory",
          "appId": "1:366878131351:android:cef8649585fb60acb255d2",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "j-factory",
          "appId": "1:366878131351:ios:cc41085b59a7c48fb255d2",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "macos": {
        "default": {
          "projectId": "j-factory",
          "appId": "1:366878131351:ios:cc41085b59a7c48fb255d2",
          "uploadDebugSymbols": false,
          "fileOutput": "macos/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "j-factory",
          "configurations": {
            "android": "1:366878131351:android:cef8649585fb60acb255d2",
            "ios": "1:366878131351:ios:cc41085b59a7c48fb255d2",
            "macos": "1:366878131351:ios:cc41085b59a7c48fb255d2",
            "web": "1:366878131351:web:281310f7e1a05d72b255d2",
            "windows": "1:366878131351:web:058ba8cb0d3c36cab255d2"
          }
        }
      }
    }
  }
}
